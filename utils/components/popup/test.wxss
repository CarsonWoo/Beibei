  background: linear-gradient(to right, #1ae8c8, #4a8aff);
  -webkit-background-clip: text;
  background-clip: text;
  color: transparent;
  font-family: 'Times New Roman', Times, serif;
	font-size: 30rpx;
	font-weight: bold;
	font-stretch: normal;
	letter-spacing: 0rpx;
  text-align: center;
  position: absolute;
  bottom: 33rpx;
  right:0rpx;
}

